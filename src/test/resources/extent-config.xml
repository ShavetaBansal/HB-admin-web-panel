<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
  <configuration>

   <!-- document encoding -->
<!-- defaults to UTF-8 -->
<encoding>UTF-8</encoding>

<!-- protocol for script and stylesheets -->
<!-- defaults to https -->
<protocol>https</protocol>

<!-- title of the document -->
<documentTitle>HB Scale Web Testing</documentTitle>

<!-- report name - displayed at top-nav -->
<reportName>HB Scale Web Testing</reportName>

<reportName1>
	<image id="image-zoom">
		<![CDATA[	
		<img src='https://memorres.com/wp-content/uploads/2022/06/Desktop-logo.svg'/>
		]]>
	</image>
	
</reportName1>


<!-- report headline - displayed at top-nav, after reportHeadline -->
<reportHeadline>HB Scale Web Testing</reportHeadline>

<!-- global date format override -->
<!-- defaults to yyyy-MM-dd -->
<dateFormat>yyyy-MM-dd</dateFormat>

<!-- global time format override -->
<!-- defaults to HH:mm:ss -->
<timeFormat>HH:mm:ss</timeFormat>

<!-- custom javascript -->
<scripts>
  <![CDATA[
    $(document).ready(function() {
      $("#image-zoom").src("src",src);
	  $("#image-zoom").append("src",src);
    });
  ]]>
</scripts>

<!-- custom styles -->
<styles>
   <![CDATA[
            .report-name { padding-left: 120px; } .report-name > img { float: left;height: 90%;margin-left: 120px;margin-top: 2px;width: auto; }
        ]]>
